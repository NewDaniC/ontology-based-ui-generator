@base <http://www.cedri.com/SmartLEM-EC-View#> .

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix md:  <http://www.cedri.com/OntologyToAPI-Metadata#> .
@prefix bm:  <http://www.cedri.com/OntologyToAPI-BusinessModel#> .
@prefix ui:  <http://www.cedri.com/OntologyToUI-UserInterface#> .

@prefix EC_View: <http://www.cedri.com/SmartLEM-EC-View#> .
@prefix ASE_BM: <http://www.cedri.com/SmartLEM-ASE-BMs#> .

<http://www.cedri.com/SmartLEM-EC-View> rdf:type owl:Ontology ;
                                                           owl:versionIRI <http://www.cedri.com/SmartLEM-EC-View/1.0.0> ;
                                                           owl:imports <http://www.cedri.com/OntologyToAPI-Metadata/1.0.0> ,
                                                                       <http://www.cedri.com/OntologyToAPI-BusinessModel/1.0.0> ,
                                                                       <http://www.cedri.com/OntologyToUI-UserInterface/1.0.0> ,
                                                                       <http://www.cedri.com/SmartLEM-ASE-BMs/1.0.0> ;
                                                           <http://purl.org/dc/terms/creator> "https://orcid.org/0000-0002-7620-0960" ;
                                                           <http://purl.org/dc/terms/description> "This ontology represent the User Interface to a Local Electricity Market" ;
                                                           <http://purl.org/dc/terms/issued> "2025-04-07" ;
                                                           <http://purl.org/dc/terms/modified> "2025-11-19" .

#################################################################
#    Classes
#################################################################

EC_View:RunSimulation rdf:type owl:Class ;
                           rdfs:subClassOf ui:Page ;
                           rdfs:comment "A page to user to run a Local Energy Community simulation." .

#################################################################
#    Individuals
#################################################################

EC_View:RunSimulation_Page rdf:type EC_View:RunSimulation ;
                            ui:hasGraphicRepresentation EC_View:Label1 ,
                                                        EC_View:Table1 .

EC_View:Label1 rdf:type ui:Label ;
                  ui:hasText "Get Energy Community Information" .

EC_View:Label2 rdf:type ui:Label ;
                  ui:hasText "Get Energy Community Information" .

EC_View:Label3 rdf:type ui:Label ;
                  ui:hasText "Get Energy Community Information" .

EC_View:Label4 rdf:type ui:Label ;
                  ui:hasText "111111111" .

EC_View:Table1 rdf:type ui:Table ;
                  ui:hasBusinessModel ASE_BM:SimulationBM .