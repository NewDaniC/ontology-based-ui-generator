#################################################################
#    Classes
#################################################################

EC_Operations:CreateEnergyCommunityBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation create a Local Electricity Market." .

EC_Operations:AddMemberBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to add members to a Local Electricity Market." .

EC_Operations:AddAssetBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to add assets to a member of a Local Electricity Market." .

EC_Operations:GetMemberBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get members to a Local Electricity Market." .

EC_Operations:GetEnergyCommunityBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get all data from a Local Electricity Market." .

EC_Operations:SimulateEnergyCommunityBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to simulate all assets from all members from a Local Electricity Market." .

EC_Operations:GetLastEnergyCommunityConsProdBalanceBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get the consumption, production and net balance from the last simulation at a Local Electricity Market." .

EC_Operations:GetLastEnergyCommunityMarketBalanceBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get the billing information such as revenue, payment information from the last simulation at a Local Electricity Market." .

EC_Operations:GetMemberConsProdDeficitSurplusBalanceBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get the deficit, surplus, power, consumption, production and net balance from the last simulation at a Local Electricity Market." .


EC_Operations:GetMemberAssetOperationBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get the asset operation of a given member asset from the last simulation at a Local Electricity Market." .

EC_Operations:GetWeatherBM rdf:type owl:Class ;
                           rdfs:subClassOf bm:BusinessModel ;
                           rdfs:comment "A Energy Community operation to get weather info of a Local Electricity Community." .

