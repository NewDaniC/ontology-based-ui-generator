# Endpoint Parameters

EC_Operations:EC_Name rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "ECName" ;
                            bm:hasParameterType "str" .

EC_Operations:EC_Latitude rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "Latitude" ;
                            bm:hasParameterType "float" .

