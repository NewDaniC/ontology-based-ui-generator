                                         bm:Parameter ;
                            bm:hasParameterLabel "Longitude" ;
                            bm:hasParameterType "float" .

EC_Operations:EC_TOU rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "TOU" ;
                            bm:hasParameterType "float" .

EC_Operations:EC_FIT rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "FIT" ;
                            bm:hasParameterType "float" .

EC_Operations:EC_ID rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "EnergyCommunityID" ;
                            bm:hasParameterType "int" .

EC_Operations:MemberData rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "MemberData" ;
                            bm:hasParameterType "UploadFile" .

EC_Operations:Member_ID rdf:type owl:NamedIndividual ,
                                         bm:Parameter ;
                            bm:hasParameterLabel "MemberID" ;
