                             bm:hasParameter EC_Operations:EC_Name ,
                                             EC_Operations:EC_Latitude ,
                                             EC_Operations:EC_Longitude ,
                                             EC_Operations:EC_TOU ,
                                             EC_Operations:EC_FIT .

EC_Operations:CreateEnergyCommunityFunction rdf:type owl:NamedIndividual ,
                                       excode:ExternalCode ;
                               excode:hasFunction "CreateEnergyCommunity" ;
                               excode:hasPythonFile "./BMs_Wrappers/EC_BMs.py" ;
                               excode:requiresLib "sqlite3" ,
                                                  "pathlib" .

